{
  "name": "llm-boilerplate",
  "version": "1.0.0",
  "description": "File-driven specification system for LLM-assisted development. From idea to working prototype through structured dialogue.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --config core/app/vite.config.ts",
    "build": "tsx core/builder/build.ts && vite build --config core/app/vite.config.ts",
    "preview": "vite preview --config core/app/vite.config.ts",
    "lint": "eslint core/app/src --ext .ts,.tsx",
    "typecheck": "tsc -p core/app/tsconfig.json --noEmit",
    "prisma:validate": "prisma validate --schema=src/prisma/schema.prisma",
    "prisma:format": "prisma format --schema=src/prisma/schema.prisma"
  },
  "keywords": [
    "llm",
    "boilerplate",
    "specification",
    "ai",
    "prototype",
    "generator",
    "toml",
    "react",
    "documentation"
  ],
  "author": "Artyom Gorlovetskiy (unbywyd) <https://unbywyd.com>",
  "license": "CC-BY-NC-4.0",
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
